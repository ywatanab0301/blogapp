<main>
  <div class="contents">
    <%= @articles.each do |article| %>
        <div class="input-field col s6">
          <div class="card-panel hoverable">
            <h6><%= article.title %></h6>
            <p><%= simple_format(article.content) %></p>
          </div>
        </div>
    <% end %>
  </div>
</main>
